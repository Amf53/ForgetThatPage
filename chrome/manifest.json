{
  "manifest_version": 2,

  "name":  "Forget that page",
  "description": "Delete the current webpage from your browsing history, just with one click.",
  "version": "1.2.1",

  "browser_action": {
  "default_icon": "img/swipe-icon.png",
  "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["js/background.js"]
  },

  "permissions": [
    "history",
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
